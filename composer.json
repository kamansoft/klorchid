{
  "name": "kamansoft/klorchid",
  "description": "A orchid/platform package to speed up Kamansoft backofice php applications ",
  "type": "library",
  "require": {
    "php": "^7.4|^8.0",
    "laravel-lang/lang": "~8.0",
    "datakrama/eloquid": "^2.0",
    "kamansoft/platform-multiorg": "dev-main"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.5"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "lemys lopez",
      "email": "lemyskaman@gmail.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Kamansoft\\Klorchid\\KlorchidServiceProvider"
      ],
      "aliases": {
        "Notificator": "Kamansoft\\Klorchid\\Support\\Facades\\Notificator"
      }
    }
  },
  "autoload": {
    "psr-4": {
      "Kamansoft\\Klorchid\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  }
}

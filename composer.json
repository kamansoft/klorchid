{
    "name": "kamansoft/klorchid",
    "description": "A orchid/platform package to speed up Kamansoft backofice php applications ",
    "type": "library",
    "require": {
        "php": "^7.4|^8.0",
        "doctrine/dbal": "^3.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.12",
        "laravel/jetstream": "^2.1",
        "laravel/sanctum": "^2.6",
        "laravel/tinker": "^2.5",
        "livewire/livewire": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "lemys lopez",
            "email": "lemyskaman@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Kamansoft\\Klorchid\\KlorchidServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
          "Kamansoft\\Klorchid\\": "src"
        }
      },
         "repositories": [
        {
            "type": "path",
            "url": "../../orchidsoftware/platform",
            "symlink": true
        }
    ] 


}
